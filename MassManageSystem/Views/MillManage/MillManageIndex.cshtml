
@{
    ViewBag.Title = "MillManageIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Mill Manage Information</h2>


<div class="well">
    <a href="@Url.Action("AddTodayMill","MillManage")" class="btn btn-primary">Add Today Mill</a>
    <a href="@Url.Action("AllMill","MillManage")" class="btn btn-primary">All Mill</a>
    @*<a href="#" class="btn btn-primary" onclick="AddEditCourse(0)">New</a>*@
</div>



<div class="modal body-content" id="myModal">
    <div class="modal-body">
        <div class="modal-content">
            <div class="modal-header">
                <a href="#" class="close" data-dismiss="modal">&times;</a>
                <h3 class="modal-title">Add Today Mills</h3>
            </div>
            <div class="modal-body" id="myModalBodyDiv">


            </div>
            <div class="modal-footer">
                <a href="#" class="btn btn-default" data-dismiss="modal">Cancel</a>

            </div>

        </div>

    </div>

</div>

@*This month mill*@
<table class="display" id="MyDataTable">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Morning
            </th>
            <th>
                Lunch
            </th>
            <th>
                Dinner
            </th>
            <th>
                Date
            </th>
            <th>

            </th>


        </tr>
    </thead>

    <tbody></tbody>

</table>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/MillManage.js"></script>
    <script src="~/Scripts/DataTable/js/jquery.dataTables.js"></script>
    <link href="~/Content/DataTable/css/jquery.dataTables.css" rel="stylesheet" />
}